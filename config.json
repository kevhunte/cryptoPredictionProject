{
    "prep": {
        "n_steps": 50,
        "scale": true,
        "shuffle": true,
        "lookup_step": 1,
        "split_by_date": true,
        "test_size": 0.2, 
        "feature_columns": ["Close", "Volume", "Open", "High", "Low"]
    },
    "create": {
        "sequence_length": 50,
        "n_features": 5,
        "units": 256,
        "n_layers": 2, 
        "dropout": 0.3,
        "loss": "mean_absolute_error", 
        "optimizer": "rmsprop", 
        "bidirectional": false,
        "activation": "linear"
    },
    "train": {
        "output_dir": "output",
        "BATCH_SIZE": 64,
        "EPOCHS": 10
    },
    "test": {}
}
